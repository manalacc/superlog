generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = file:./dev.db
}

model Entry {
  id        Int      @id @default(autoincrement())
  title     String
  image     String?
  startDate DateTime
  finishDate DateTime?
  completed Boolean
  timeSpent Float
  type      String
  ranking   String
}